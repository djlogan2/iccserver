{
  "plugins": [
    "meteor"
  ],
  "extends": [
    "plugin:meteor/recommended",
    "react-app",
    "plugin:prettier/recommended"
  ],
  "env": {
    "node": true,
    "meteor": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "globals": {
    "BigInt": true
  },
  "rules": {
    "no-console": "warn"
  }
}
